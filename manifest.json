{
  "manifest_version": 2,
  "name": "Lululemon Analytics Reader",
  "version": "1.0",
  "description": "Reads the window.analytics object on Lululemon's website.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}